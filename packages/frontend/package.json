    "postcss-easy-import": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-dropzone": "^10.1.5",
    "tailwindcss": "^1.0.4",
    "yup": "^0.27.0"
  },
  "resolutions": {
    "c21e": "1.1.9"
  },
  "lint-staged": {
